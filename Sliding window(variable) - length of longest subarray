"""
Given an array of non-negative integers and an integer k, 
find the length of the longest contiguous subarray whose sum is less than or equal to K.
"""

class Solution(object):
    def longest_subarray(self, nums,k):
        """
        :type prices: List[int]
        :rtype: int
        """
        l=r=res=count=0
        max_count=float('-inf')
        # max_l_r=[l,r]
        while r<len(nums):
            res+=nums[r]
            count+=1
            if res>k:
                res-=nums[l]
                l+=1
                count-=1
            max_count=max(max_count,count)
            r+=1
        return max_count

obj = Solution()
nums = [4,1,1,1,2,3]
k=5
print(obj.longest_subarray(nums,k))
